# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'SocialNetwork' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  inhibit_all_warnings!

  # Pods for SocialNetwork
  pod 'FacebookCore', '~> 0.9.0'
  pod 'FacebookLogin', '~> 0.9.0'
  pod 'FacebookShare', '~> 0.9.0'

  pod 'Firebase/Core', '~> 6.10.0'
  pod 'Firebase/Auth', '~> 6.10.0'
  pod 'Firebase/Database', '~> 6.10.0'
  pod 'Firebase/Messaging', '~> 6.10.0'
  pod 'Firebase/Storage', '~> 6.10.0'
  pod 'Firebase/Firestore', '~> 6.10.0'

  pod 'Alamofire', '~> 4.9.0'
  pod 'Kingfisher', '~> 5.8.2'

  pod 'Eureka'

end
